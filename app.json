{
  "expo": {
    "name": "ChatKita",
    "slug": "chatkita",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "simplechat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.chatkita.app",
      "adaptiveIcon": {
      "foregroundImage": "./assets/logo.png",
      "backgroundColor": "#FFFFFF"
  }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0babc2d9-c120-4323-ab39-1bf198d7cc4a"
      }
    }
  }
}